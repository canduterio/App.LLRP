<?xml version="1.0" encoding="UTF-8"?>
<LLRPServerInstanceConfiguration
	xmlns="urn:havis:llrp:server:configuration:xsd:1" instanceId="test">
	<persistence>
		<times>
			<manual>false</manual>
			<afterChanges>true</afterChanges>
		</times>
		<groupId>
			<baseFormat>dd</baseFormat>
			<suffix>
				<serialNumber>true</serialNumber>
			</suffix>
		</groupId>
		<cleanUp>
			<times>
				<manual>true</manual>
				<afterFlush>false</afterFlush>
			</times>
			<maxCreationDateInterval>3</maxCreationDateInterval>
		</cleanUp>
		<entities>
			<entity>
				<className>havis.llrpservice.xml.configuration.LLRPServerInstanceConfigurationType
				</className>
				<output>
					<file>
						<baseDir>serverInstanceConfig</baseDir>
						<type>
							<XML>
								<encoding>UTF8</encoding>
							</XML>
						</type>
					</file>
				</output>
				<times>
					<manual>true</manual>
					<afterChanges>true</afterChanges>
				</times>
				<groupId>
					<baseFormat>HH</baseFormat>
					<suffix>
						<serialNumber>true</serialNumber>
					</suffix>
				</groupId>
				<cleanUp>
					<times>
						<manual>false</manual>
						<afterFlush>false</afterFlush>
					</times>
					<maxCreationDateInterval>4</maxCreationDateInterval>
				</cleanUp>
			</entity>
		</entities>
	</persistence>
</LLRPServerInstanceConfiguration>
